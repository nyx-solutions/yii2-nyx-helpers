{
  "name": "nyx-solutions/yii2-nyx-helpers",
  "description": "Yii2 NYX Helpers",
  "type": "library",
  "keywords": ["yii2", "extension", "helpers", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "4.0.0",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-iconv": "*",
    "nyx-solutions/yii2-nyx": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "nyx\\helpers\\": "helpers/"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nyx-solutions/yii2-nyx-helpers"
    }
  ]
}
